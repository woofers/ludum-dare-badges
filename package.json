{
  "name": "ludum-dare-badges",
  "version": "1.1.0",
  "description": "SVG & PNG badges for Ludum Dare Game Jam results",
  "scripts": {
    "build:react": "yarn --cwd packages/react build",
    "build:demo": "yarn --cwd packages/demo local && yarn --cwd packages/demo build",
    "build": "NODE_ENV=production yarn build:react && yarn build:demo",
    "install:api": "yarn --cwd packages/api install",
    "install:react": "yarn --cwd packages/react install",
    "install:demo": "yarn --cwd packages/demo install",
    "install": "yarn install:api && yarn install:react && yarn install:demo",
    "clean:react": "yarn --cwd packages/react clean",
    "clean:demo": "yarn --cwd packages/demo clean",
    "clean": "yarn clean:react && yarn clean:demo",
    "watch": "NODE_ENV=development now dev",
    "dev": "NODE_ENV=development now dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woofers/ludum-dare-badges.git"
  },
  "bugs": {
    "url": "https://github.com/woofers/ludum-dare-badges/issues"
  },
  "engines": {
    "node": "12.x"
  },
  "homepage": "https://github.com/woofers/ludum-dare-badges#readme",
  "author": "Jaxson Van Doorn <jaxson.vandoorn@gmail.com>",
  "license": "MIT",
  "dependencies": {}
}
